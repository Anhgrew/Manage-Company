
public class Truongphong extends Nhanvien {
	protected String poSiTion;

	public Truongphong(String maNV, String tenNV, int namSinh, String email, String soDT, String maPB, String string, int value) {
		super(maNV, tenNV, namSinh, email, soDT, maPB, value);
		poSiTion = string;
	}

	public String getPoSiTion() {
		return poSiTion;
	}

	public void setPoSiTion(String poSiTion) {
		this.poSiTion = poSiTion;
	}

}