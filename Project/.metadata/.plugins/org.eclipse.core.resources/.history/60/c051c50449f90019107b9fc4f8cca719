import java.util.LinkedList;

public class Truongphong extends Nhanvien {
	protected String poSiTion;

	public Truongphong(String maNV, String tenNV, int namSinh, String email, String soDT, String maPB,
			LinkedList<Nhiemvu> dsNhiemVu, String string) {
		super(maNV, tenNV, namSinh, email, soDT, maPB, dsNhiemVu);
		this.poSiTion = string;
		// TODO Auto-generated constructor stub
	}

	public Truongphong(String maNV, String tenNV, int namSinh, String email, String soDT, String maPB, String string) {
		super(maNV, tenNV, namSinh, email, soDT, maPB);
		this.poSiTion = string;
	}

	public String getPoSiTion() {
		return poSiTion;
	}

	public void setPoSiTion(String poSiTion) {
		this.poSiTion = poSiTion;
	}

}