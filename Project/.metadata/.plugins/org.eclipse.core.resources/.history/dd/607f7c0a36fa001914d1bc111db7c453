
public class XuLy {

	public static void main(String[] args) {
		Congty a=new Congty();
		a.nhapFile();
		//Tim danh sach nhan vien lam nhieu nhiem vu nhat cong ty
		a.nhanVienNhieuNVNhat();
		//Tim danh sach nhan tre nhat va lam nhieu nhiem viec nhat cong ty
		a.nhanVienTreNhatDangNhieuNVNhat();
		//Tim phong ban co nhan vien nhan tre nhat
		a.phongBanCoNVTreNhat();
		//Xoa nhan vien co id cho truoc
		a.xoaMotNhanVien("011");
		
		a.Xuat();
		//Nhan vien sap xep theo thu tu lam nhieu nhiem vu o tren
		a.sapXepNhanVienTheoNhiemVu();
		a.tinhSoNVDuoiQuyen();
		//Tinh luong 1 nhan vien duoc nhap maNV truoc
		a.tinhLuongNhanVien("005");
		System.out.println("____________________________________________");
		//Tinh luong cong ty can chi tra
		a.tinhLuongCongTy();
	}	

}
